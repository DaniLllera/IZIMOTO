<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/main_graph"
    app:startDestination="@id/paginaPrincipalFragment">
    <fragment
        android:id="@+id/splashScreamFragment"
        android:name="com.example.proyectom07_izimoto.SplashScreamFragment"
        android:label="fragment_splash_scream"
        tools:layout="@layout/fragment_splash_scream" >
        <action
            android:id="@+id/action_splashScreamFragment_to_loginFragment"
            app:destination="@id/loginFragment" />
    </fragment>
    <fragment
        android:id="@+id/loginFragment"
        android:name="com.example.proyectom07_izimoto.LoginFragment"
        android:label="fragment_login"
        tools:layout="@layout/fragment_login" >
        <action
            android:id="@+id/action_loginFragment_to_registroFragment"
            app:destination="@id/registroFragment" />
        <action
            android:id="@+id/action_loginFragment_to_paginaPrincipalFragment"
            app:destination="@id/paginaPrincipalFragment" />
    </fragment>
    <fragment
        android:id="@+id/registroFragment"
        android:name="com.example.proyectom07_izimoto.RegistroFragment"
        android:label="fragment_registro"
        tools:layout="@layout/fragment_registro" >
        <action
            android:id="@+id/action_registroFragment_to_paginaPrincipalFragment"
            app:destination="@id/paginaPrincipalFragment" />
    </fragment>
    <fragment
        android:id="@+id/paginaPrincipalFragment"
        android:name="com.example.proyectom07_izimoto.PaginaPrincipalFragment"
        android:label="Seleccione el Permiso"
        tools:layout="@layout/fragment_pagina_principal" >
        <action
            android:id="@+id/action_paginaPrincipalFragment_to_permisoMotoFragment"
            app:destination="@id/permisoMotoFragment" />
        <action
            android:id="@+id/action_paginaPrincipalFragment_to_permisoCamionFragment"
            app:destination="@id/permisoCamionFragment" />
        <action
            android:id="@+id/action_paginaPrincipalFragment_to_permisoCocheFragment"
            app:destination="@id/permisoCocheFragment" />
    </fragment>
    <fragment
        android:id="@+id/permisoMotoFragment"
        android:name="com.example.proyectom07_izimoto.PermisoMotoFragment"
        android:label="Permiso A1/A2"
        tools:layout="@layout/fragment_permiso_moto" >
        <action
            android:id="@+id/action_permisoMotoFragment_to_testTemasFragment"
            app:destination="@id/testTemasFragment" />
    </fragment>
    <fragment
        android:id="@+id/cerrarSesionFragment"
        android:name="com.example.proyectom07_izimoto.CerrarSesionFragment"
        android:label="fragment_cerrar_sesion"
        tools:layout="@layout/fragment_cerrar_sesion" >
        <action
            android:id="@+id/action_cerrarSesionFragment_to_loginFragment"
            app:destination="@id/loginFragment" />
    </fragment>
    <fragment
        android:id="@+id/infoAppFragment"
        android:name="com.example.proyectom07_izimoto.InfoAppFragment"
        android:label="Información de la app"
        tools:layout="@layout/fragment_info_app" />
    <fragment
        android:id="@+id/testTemasFragment"
        android:name="com.example.proyectom07_izimoto.TestTemasFragment"
        android:label="Test por temas Motos"
        tools:layout="@layout/fragment_test_temas" >
        <action
            android:id="@+id/action_testTemasFragment_to_testSenalizacionFragment"
            app:destination="@id/testSenalizacionFragment" />
    </fragment>
    <fragment
        android:id="@+id/resultadosFragment"
        android:name="com.example.proyectom07_izimoto.ResultadosFragment"
        android:label="Tus Resultados"
        tools:layout="@layout/fragment_resultados" />
    <fragment
        android:id="@+id/fechaExamenFragment"
        android:name="com.example.proyectom07_izimoto.FechaExamenFragment"
        android:label="Fechas de Examen"
        tools:layout="@layout/fragment_fecha_examen" />
    <fragment
        android:id="@+id/testTemas2Fragment2"
        android:name="com.example.proyectom07_izimoto.TestTemas2Fragment"
        android:label="Test por temas Camión"
        tools:layout="@layout/fragment_test_temas2" >
        <action
            android:id="@+id/action_testTemas2Fragment2_to_testSenalizacionFragment"
            app:destination="@id/testSenalizacionFragment" />
    </fragment>
    <fragment
        android:id="@+id/permisoCamionFragment"
        android:name="com.example.proyectom07_izimoto.PermisoCamionFragment"
        android:label="Permiso C1/C"
        tools:layout="@layout/fragment_permiso_camion" >
        <action
            android:id="@+id/action_permisoCamionFragment_to_testTemas2Fragment2"
            app:destination="@id/testTemas2Fragment2" />
    </fragment>
    <fragment
        android:id="@+id/permisoCocheFragment"
        android:name="com.example.proyectom07_izimoto.PermisoCocheFragment"
        android:label="Permiso B"
        tools:layout="@layout/fragment_permiso_coche" >
        <action
            android:id="@+id/action_permisoCocheFragment_to_testTemas3Fragment"
            app:destination="@id/testTemas3Fragment" />
    </fragment>
    <fragment
        android:id="@+id/testTemas3Fragment"
        android:name="com.example.proyectom07_izimoto.TestTemas3Fragment"
        android:label="Test por temas Camión"
        tools:layout="@layout/fragment_test_temas3" >
        <action
            android:id="@+id/action_testTemas3Fragment_to_testSenalizacionFragment"
            app:destination="@id/testSenalizacionFragment" />
    </fragment>
    <fragment
        android:id="@+id/testSenalizacionFragment"
        android:name="com.example.proyectom07_izimoto.TestSenalizacionFragment"
        android:label="Señalización"
        tools:layout="@layout/fragment_test_senalizacion" >
        <action
            android:id="@+id/action_testSenalizacionFragment_to_testOficialesDGTFragment"
            app:destination="@id/testOficialesDGTFragment" />
    </fragment>
    <fragment
        android:id="@+id/testOficialesDGTFragment"
        android:name="com.example.proyectom07_izimoto.TestOficialesDGTFragment"
        android:label="Oficiales DGT"
        tools:layout="@layout/fragment_test_oficiales_d_g_t" >
        <action
            android:id="@+id/action_testOficialesDGTFragment_to_testOficialesDGTTiempoFragment"
            app:destination="@id/testOficialesDGTTiempoFragment" />
    </fragment>
    <fragment
        android:id="@+id/testOficialesDGTTiempoFragment"
        android:name="com.example.proyectom07_izimoto.TestOficialesDGTTiempoFragment"
        android:label="Test Oficiales Con Tiempo"
        tools:layout="@layout/fragment_test_oficiales_d_g_t_tiempo" >
        <action
            android:id="@+id/action_testOficialesDGTTiempoFragment_to_testPoliglotasFragment"
            app:destination="@id/testPoliglotasFragment" />
    </fragment>
    <fragment
        android:id="@+id/testPoliglotasFragment"
        android:name="com.example.proyectom07_izimoto.TestPoliglotasFragment"
        android:label="Test para Políglotas"
        tools:layout="@layout/fragment_test_poliglotas" >
        <action
            android:id="@+id/action_testPoliglotasFragment_to_resultadosDelTestFragment"
            app:destination="@id/resultadosDelTestFragment" />
    </fragment>
    <fragment
        android:id="@+id/resultadosDelTestFragment"
        android:name="com.example.proyectom07_izimoto.ResultadosDelTestFragment"
        android:label="Resultados del Test"
        tools:layout="@layout/fragment_resultados_del_test" >
        <action
            android:id="@+id/action_resultadosDelTestFragment_to_paginaPrincipalFragment"
            app:destination="@id/paginaPrincipalFragment" />
    </fragment>
    <fragment
        android:id="@+id/registroProfessorFragment"
        android:name="com.example.proyectom07_izimoto.RegistroProfessorFragment"
        android:label="fragment_registro_professor"
        tools:layout="@layout/fragment_registro_professor" >
        <action
            android:id="@+id/action_registroProfessorFragment_to_paginaPrincipalProfessorFragment"
            app:destination="@id/paginaPrincipalProfessorFragment" />
    </fragment>
    <fragment
        android:id="@+id/paginaPrincipalProfessorFragment"
        android:name="com.example.proyectom07_izimoto.PaginaPrincipalProfessorFragment"
        android:label="Panel de Control Professores"
        tools:layout="@layout/fragment_pagina_principal_professor" />

</navigation>